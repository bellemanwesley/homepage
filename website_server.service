[Unit]
Description=Django Application for My Personal Website
After=multi-user.target
Conflicts=getty@tty1.service

[Service]
Type=simple
ExecStart=/usr/bin/python3 /home/ubuntu/environment/homepage/manage.py runserver 127.0.0.1:1337
StandardInput=tty-force
User=ubuntu

[Install]
WantedBy=multi-user.target
